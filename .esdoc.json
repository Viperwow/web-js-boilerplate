{
  "source": "./src",
  "destination": "./docs",
  "includes": ["\\.js(x)?$"],
  "excludes": ["((\\.config)|index).js(x)?$"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "lint": {
          "enable": true
        },
        "coverage": {
          "enable": true
        },
        "accessor": {
          "access": [
            "public",
            "protected",
            "private"
          ],
          "autoPrivate": true
        },
        "undocumentIdentifier": {
          "enable": false
        },
        "unexportedIdentifier": {
          "enable": false
        },
        "typeInference": {
          "enable": true
        },
        "brand": {
          "title": "web-js-boilerplate",
          "description": "Boilerplate for any web project",
          "repository": "https://gitlab.com/Viperwow/web-js-boilerplate.git",
          "author": "Viperwow"
        },
        "test": {
          "source": "./src",
          "interfaces": ["describe", "it", "context", "suite", "test"],
          "includes": ["(test|spec|steps|support)\\.js(x)?$"],
          "excludes": ["((\\.config)|index).js(x)?$"]
        },
        "manual": {
          "index": "./README.md",
          "globalIndex": true,
          "files": [
            "./README.md",
            "./CHANGELOG.md"
          ]
        }
      }
    },
    {"name": "esdoc-react-plugin"},
    {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}},
    {"name": "esdoc-jsx-plugin", "option": {"enable": true}}
  ]
}
