{
  "plugins": [
    "transform-decorators-legacy",
    "lodash",
    "transform-strict-mode"
  ],
  "presets": [
    "jest",
    [
      "env",
      {
        "modules": false,
        "useBuiltIns": true
      }
    ],
    "react",
    "stage-0"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "test": {
      "presets": [
        "jest",
        [
          "env",
          {
            "useBuiltIns": true
          }
        ],
        "react",
        "stage-0"
      ]
    }
  }
}
