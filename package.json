{
  "name": "web-js-boilerplate",
  "version": "0.0.4",
  "description": "Boilerplate for any web project",
  "main": "index.jsx",
  "readme": "./README.md",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG_ENV=true BABEL_ENV=development NODE_ENV=development webpack-dev-server --progress --env.env=development",
    "start:storybook": "cross-env DEBUG_ENV=true BABEL_ENV=development NODE_ENV=development start-storybook -p 9001 -c ./configs/storybook",
    "build:rc": "cross-env DEBUG_ENV=true BABEL_ENV=production NODE_ENV=production webpack --progress --env.env=rc",
    "build:prod": "cross-env DEBUG_ENV=false BABEL_ENV=production NODE_ENV=production webpack --progress --env.env=production",
    "build:storybook": "yarn test:jest:output && cross-env DEBUG_ENV=false BABEL_ENV=production NODE_ENV=production build-storybook -c ./configs/storybook",
    "test": "yarn test:jest:output && yarn test:eslint && yarn test:stylelint && yarn test:flow",
    "test:eslint": "cross-env TIMING=1 eslint . --ext .js,.jsx,.js.flow",
    "test:stylelint": "stylelint \"**/*.(sa|sc|c)ss\" --config ./.stylelintrc.js",
    "test:flow": "flow",
    "test:jest": "jest -c ./configs/jest/jest.config.js",
    "test:jest:update": "yarn test:jest -u",
    "test:jest:output": "yarn test:jest --json --outputFile=jest-results.json",
    "test:jest:output:watch": "yarn test:jest:output --watch || true"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/viperwow-common/frontend/web-js-boilerplate.git"
  },
