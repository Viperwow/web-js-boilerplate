{
  "name": "web-js-boilerplate",
  "version": "0.0.2",
  "description": "Boilerplate for any web project",
  "main": "index.js",
  "readme": "./README.md",
  "private": true,
  "scripts": {
    "build:dev": "webpack-dev-server --progress --env.env=development",
    "build:rc": "webpack --progress --env.env=rc",
    "build:prod": "webpack --progress --env.env=production",
    "build:docs": "rimraf ./docs && esdoc",
    "test": "yarn test:eslint && yarn test:jest",
    "test:eslint": "cross-env TIMING=1 eslint . --ext .js,.jsx",
    "test:jest": "jest -c ./configs/jest/jest.config.js",
    "test:jest:update": "yarn test:jest -- -u",
    "test:gemini": "gemini test",
    "test:gemini:update": "gemini update",
    "selenium:install": "./node_modules/.bin/selenium-standalone install",
    "selenium:start": "selenium-standalone start"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/viperwow-common/frontend/web-js-boilerplate.git"
  },
  "keywords": [
    "web",
    "frontend",
    "boilerplate"
  ],
